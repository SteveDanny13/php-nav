<?php

$cars = [
    "Toyota" => ["Power" => 150, "Year" => 2020, "Fuel" => "Petrol"],
    "BMW" => ["Power" => 220, "Year" => 2018, "Fuel" => "Diesel"],
    "Tesla" => ["Power" => 300, "Year" => 2022, "Fuel" => "Electric"]
];

$keys = array_keys($cars);

for ($i = 0; $i < count($cars); $i++) {
    echo $keys[$i] . "<br>";
    foreach ($cars[$keys[$i]] as $key => $value) {
        echo $key . " : " . $value . "<br>";
    }

    echo "<br>";
}
?>
